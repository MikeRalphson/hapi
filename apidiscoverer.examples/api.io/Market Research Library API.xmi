<?xml version="1.0" encoding="ASCII"?>
<swagger:Api xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:swagger="http://swagger.io/core" swagger="2" host="api.trade.gov" basePath="/">
  <info title="Trade.gov Market Research Library" description="This API provides metadata for country and industry reports that are produced by ITAu2019s trade experts and are available in ITAu2019s online market research library. " version="1.0"/>
  <schemes>http</schemes>
  <produces>application/json</produces>
  <paths pattern="/market_research_library/search">
    <get summary="Market Research Library Search" description="This API provides metadata for country and industry reports that are produced by ITAs trade experts and are available in ITAs online market research library." operationId="research">
      <tags>Trade</tags>
      <tags> Commerce</tags>
      <tags> Reports</tags>
      <tags> Research</tags>
      <parameters name="keyword" description="Returns market research reports for a match in the description or title fields." required="true" type="string"/>
      <parameters name="countries" description="Returns office locations based on ISO alpha-2 country codes." required="true" type="string"/>
      <parameters name="industry" description="Returns market research reports for a specific industry." required="true" type="string"/>
      <parameters name="size" description="The size parameter allows you to configure the maximum amount of hits to be returned." required="true" type="string"/>
      <parameters name="offset" description="The offset parameter defines the offset from the first result you want to fetch." required="true" type="string"/>
      <responses code="200"/>
    </get>
  </paths>
  <definitions name="Research">
    <properties description="" name="id"/>
    <properties description="" name="countries"/>
    <properties description="" name="description"/>
    <properties description="" name="expiration_date"/>
    <properties description="" name="industry"/>
    <properties description="" name="report_type"/>
    <properties description="" name="title"/>
  </definitions>
</swagger:Api>
