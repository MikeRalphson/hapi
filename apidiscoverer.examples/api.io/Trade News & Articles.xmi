<?xml version="1.0" encoding="ASCII"?>
<swagger:Api xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:swagger="http://swagger.io/core" swagger="2" host="api.trade.gov" basePath="/">
  <info title="International Trade Administration Trade News &amp; Articles" description="The Trade News &amp; Articles API provides in-depth news and articles written by Trade Specialists working in the Federal government. The news and articles are categorized by industry, country, and topic area. Developers can use this API to provide exporters with the latest insights into their industry and into the countries with which they would like to do business." version="1.0"/>
  <schemes>http</schemes>
  <produces>application/json</produces>
  <paths pattern="/trade_articles/search">
    <get summary="Trade News And Articles" description="The Trade News &amp; Articles API provides in-depth news and articles written by Trade Specialists working in the Federal government. The news and articles are categorized by industry, country, and topic area. Developers can use this API to provide exporters with the latest insights into their industry and into the countries with which they would like to do business." operationId="news">
      <tags>Government</tags>
      <tags> Trade</tags>
      <tags> News</tags>
      <tags> Articles</tags>
      <tags> Industries</tags>
      <parameters name="keyword" description="Returns articles for a match in the content and title fields." required="true" type="string" format="string"/>
      <parameters name="evergreen" description="Flag to designate articles that are always relevant to particular topic." type="boolean" format="boolean"/>
      <parameters name="pub_date_start" description="Returns articles created on or after a specific date." type="string" format="date"/>
      <parameters name="pub_date_end" description="Returns articles created on or before a specified date." type="string" format="date"/>
      <parameters name="update_date" description="Returns articles created on a specified date." type="string" format="date"/>
      <parameters name="size" description="The size parameter allows you to configure the maximum amount of hits to be returned. " required="true" type="string" format="string"/>
      <parameters name="offset" description="The offset parameter defines the offset from the first result you want to fetch." required="true" type="string" format="string"/>
      <responses code="200"/>
    </get>
  </paths>
  <definitions name="Article">
    <properties description="Unique identifier for post" type="string" name="id"/>
    <properties description="Flag to designate u201ctimelessu201d articles, always topically relevant" type="string" name="evergreen"/>
    <properties description="Body of the article" type="string" name="content"/>
    <properties description="Earliest date article can be published (default sort)" type="string" name="pub_date"/>
    <properties description="Title of the article" type="string" name="title"/>
    <properties description="Date the article was last updated" type="string" name="update_date"/>
    <properties description="Type of content embodied by the article" type="string" name="content_type"/>
    <properties description="Phase of exporting that a business should be in to benefit from the article" type="string" name="export_phase"/>
    <properties description="Industry category assigned to this article" type="string" name="industry"/>
    <properties description="Topic assigned to this article" type="string" name="topic"/>
    <properties description="Subtopic assigned to this article" type="string" name="subtopic"/>
    <properties description="Trade region covered by an articleu2019s content" type="string" name="trade_region"/>
    <properties description="World region covered by an articleu2019s content" type="string" name="geo_region"/>
    <properties description="Would sub-region covered by an articleu2019s content" type="string" name="geo_subregion"/>
    <properties description="Country covered by an articleu2019s content" type="string" name="country"/>
    <properties description="Keywords assigned by the articleu2019s author" type="string" name="keyword"/>
  </definitions>
</swagger:Api>
